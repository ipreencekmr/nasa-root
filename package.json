{
  "name": "nasa-root",
  "version": "1.0.0",
  "description": "",
  "contributors": [],
  "scripts": {
    "start": "one-app-runner",
    "start:test": "one-app-runner-test",
    "prebuild": "npm run clean",
    "build": "bundle-module",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf build",
    "prepare": "npm run build",
    "lint": "eslint --ignore-path .gitignore --ext js,jsx",
    "lint:fix": "npm run lint -- --fix",
    "test:lint": "eslint --ignore-path .gitignore --ext js,jsx,snap .",
    "test:unit": "jest",
    "test": "npm run test:lint && npm run test:unit",
    "deploy:module": "vercel --cwd build --prod --yes --name nasa-root",
    "generate:map": "node ./scripts/updateModuleMap",
    "deploy": "npm run deploy:module && npm run generate:map && vercel module_map --prod --yes --name module-map"
  },
  "one-amex": {
    "app": {
      "compatibility": "^6.4.2"
    },
    "runner": {
      "dockerImage": "oneamex/one-app-dev:latest",
      "moduleMapUrl": "https://module-map.vercel.app/module-map.json",
      "modules": [
        "."
      ],
      "rootModuleName": "nasa-root",
      "parrotMiddleware": "./dev.middleware.js"
    },
    "bundler": {
      "webpackConfigPath": "webpack.config.js",
      "providedExternals": [
        "react-intl"
      ]
    }
  },
  "dependencies": {
    "@americanexpress/one-app-ducks": "^4.3.1",
    "@americanexpress/one-app-router": "^1.1.0",
    "content-security-policy-builder": "^2.1.0",
    "fs-extra": "^11.1.1",
    "holocron": "^1.1.2",
    "immutable": "^4.0.0-rc.12",
    "node-fetch": "^2.6.11",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-intl": "^5.10.6",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@americanexpress/one-app-bundler": "^6.10.0",
    "@americanexpress/one-app-runner": "^6.10.0",
    "@babel/polyfill": "^7.12.1",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "amex-jest-preset-react": "^6.1.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-amex": "^3.5.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.15.0",
    "eslint-config-amex": "^15.3.2",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "glob": "^7.1.6",
    "husky": "^8.0.3",
    "jest": "^26.6.3",
    "jest-json-schema": "^2.1.0",
    "parrot-middleware": "^4.1.1",
    "rimraf": "^3.0.2"
  },
  "jest": {
    "preset": "amex-jest-preset-react",
    "setupFilesAfterEnv": [
      "./test-setup.js"
    ]
  }
}
